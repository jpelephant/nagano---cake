<h1><strong>配送先編集</strong></h1>
<div class="container">
  <div class="row">
    <div class="col-md-10">
      <div class="label">
      <%= form_with model: @shipping_address, local:true do |f| %>
      <div class="label">
        <%= f.label :postal_code, "郵便番号（ハイフンなし）" %>
        <%= f.text_field :postal_code %>
      </div>
      <div class="label">
        <%= f.label :address, "住所" %>
        <%= f.text_field :address %>
      </div>
      <div class="label">
        <%= f.label :name, "宛名" %>
        <%= f.text_field :name %>
        <%= f.submit"変更内容を保存" %>
      </div>
      <% end %>
      </div>
    </div>
  </div>
</div>